public String middleTwo(String str) {
  if (str.length() == 4) {
    return str.substring(1, 3);
  } else if (str.length() == 6) {
    return str.substring(2, 4);
  } else if (str.length() == 8){
    return str.substring(3, 5);
  } else if (str.length() == 10) {
    return str.substring(4, 6);
  }
  return str;
}
